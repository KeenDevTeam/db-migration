{
  "name": "@keendev/db-migration",
  "version": "1.0.3",
  "private": false,
  "description": "Database migration tool",
  "scripts": {
    "copy-resources:migration-script": "cp -R src/migration/script dist/migration/",
    "clean": "rimraf dist/",
    "transpile": "tsc",
    "build": "run-s transpile copy-resources:migration-script",
    "rebuild": "run-s clean build",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "coverage": "nyc npm run test",
    "lint": "eslint .",
    "prepublish": "run-s lint test coverage rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:keendev-team/db-migration.git"
  },
  "author": "KeenDev Team",
  "bugs": {
    "url": "https://github.com/keendev-team/db-migration/issues"
  },
  "homepage": "https://github.com/keendev-team/db-migration#readme",
  "devDependencies": {
    "@types/async": "^3.2.3",
    "@types/chai": "^4.2.12",
    "@types/ejs": "^3.0.4",
    "@types/mocha": "^8.0.2",
    "@types/node": "^14.0.26",
    "@types/pg": "^7.14.4",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "chai": "^4.2.0",
    "eslint": "^7.7.0",
    "mocha": "^8.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@speedup/error": "^1.0.7",
    "async": "^3.2.0",
    "ejs": "^3.1.3",
    "pg": "^8.3.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage/**/*",
      "dist/**/*",
      "test/**/*",
      "**/*.d.ts"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "all": true
  }
}
